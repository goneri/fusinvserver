[project]
	description = A demo FusionInventory server based on MongoDB and Perl Dancer
[access "refs/*"]
	owner = group user/Gonéri Le Bouder (goneri)
[access "refs/for/refs/*"]
	read = group user/Gonéri Le Bouder (goneri)
	push = group user/Gonéri Le Bouder (goneri)
	label-Code-Review = -2..+2 group user/Gonéri Le Bouder (goneri)
	label-Verified = -1..+1 group user/Gonéri Le Bouder (goneri)
	rebase = group user/Gonéri Le Bouder (goneri)
	removeReviewer = group user/Gonéri Le Bouder (goneri)
	submit = group user/Gonéri Le Bouder (goneri)
[access "refs/heads/*"]
	read = group user/Gonéri Le Bouder (goneri)
	create = group user/Gonéri Le Bouder (goneri)
	push = +force group user/Gonéri Le Bouder (goneri)
	pushMerge = group user/Gonéri Le Bouder (goneri)
[access "refs/tags/*"]
	push = +force group user/Gonéri Le Bouder (goneri)
	pushTag = group user/Gonéri Le Bouder (goneri)
	pushSignedTag = group user/Gonéri Le Bouder (goneri)
